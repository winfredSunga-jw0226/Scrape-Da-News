{{#each news}}
  <div class="row">
    <div class="col-sm-12">

      <div class="panel panel-info">
        <div class="panel-heading">
          <h3 class="panel-title">
            <a class="headline-link" href="{{url}}" alt="link for {{headline}}">{{headline}}</a>
            <a class="btn btn-success save">Save Article</a>
          </h3>
        </div>
        <div class="panel-body">
          <p>{{summary}}</p>
        </div>
      </div>

    </div>
  </div>
{{/each}}




